<table>
    <caption>학원 개설강의</caption>
    <colgroup>
        <col width="10%" />
        <col width="10%" />
        <col width="*" />
        <col width="15%" />
        <col width="10%" />
        <col width="10%" />
    </colgroup>
    <thead>
    <tr>
        <th scope="col">시험</th>
        <th scope="col">선생님</th>
        <th scope="col">강의이름</th>
        <th scope="col">강의기간</th>
        <th scope="col">강의요일</th>
        <th scope="col">강의시간</th>
    </tr>
    </thead>
    <tbody>
    {{#items.length}}
        {{#items}}
            <tr>
                <td class="type">{{type}}</td>
                <td class="teacher">{{teacher}}</td>
                <td class="title"><a href="{{link}}" target="_blank">{{title}}</a></td>
                <td>{{range}}</td>
                <td>{{week}}</td>
                <td>{{time}}</td>
            </tr>
        {{/items}}
        {{/items.length}}

        {{^items.length}}
            <tr class="is_not_result">
                <td colspan="6">등록된 후기가 없습니다.</td>
            </tr>
        {{/items.length}}
    </tbody>
</table>

